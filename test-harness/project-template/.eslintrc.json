{
  "env": { "node": true, "es2021": true, "jest": true },
  "parserOptions": { "ecmaVersion": 2021 },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-console": "off",
    "semi": ["error", "always"],
    "quotes": ["error", "single", { "avoidEscape": true }]
  }
}
